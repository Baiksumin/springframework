<?xml version="1.0" encoding="UTF-8" ?>
<!-- version은 xml작성을 위한 버전! encoding은 이 xml파일의 몇 바이트로 저장할 거냐! 이 ?붙은 코드는 무조건 첫줄에 작성해줘야 한다!!! -->

<!-- 문서 유형(타입) 선언 -->
<!-- DOCTYPE은 루트가 뭐냐! -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 
 <!-- 시작태그와 끝태그는 짝꿍! -->
 <!-- 루트태그(엘리먼트)는 무조건 하나! 아래 있는 태그가 루트 태그이다!-->
 <configuration>
 
    <typeAliases>
      <typeAlias type="com.mycompany.webapp.dto.Ch14Member" alias="member"/>
      <typeAlias type="com.mycompany.webapp.dto.Ch14Board" alias="board"/>
      <typeAlias type="com.mycompany.webapp.dto.Pager" alias="pager"/>
      <typeAlias type="com.mycompany.webapp.dto.Ch16Account" alias="account"/> 
   </typeAliases>
   
</configuration>